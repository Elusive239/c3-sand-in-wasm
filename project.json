{
  "langrev": "1",
  "warnings": [ "no-unused" ],
  "dependency-search-paths": [ "lib" ],
  "dependencies": [ "stb" ],
  "authors": [ "Elusive239 <@elusive-noir.bsky.social"],
  "version": "0.1.0",
  "sources": [ "src/**", "assets/**" ],
  "test-sources": [ "test/**" ],
  "output": "build",
  "cc": "gcc",
  "exec": ["pack.c3;../src/logging.c3;../src/render_common.c3; dependencies stb"],
  "targets": {
    "proj": {
      "type": "executable",
      // "exec": ["pack.c3;../src/logging.c3;../src/render_common.c3; dependencies stb"],
    },
    "proj_test": {
      "type": "executable",
      "sources": ["assets/**" ],
      // "exec": [],
    },
    "wasm": {
      "name": "out",
      "target": "wasm32",
      "type": "executable",
      "reloc": "none",
      "link-libc": false,
      "no-entry": true, 
      "opt": "O5",
      "output": ".",
      // "exec": ["pack.c3;../src/logging.c3;../src/render_common.c3; dependencies stb"],
    }
  },
  "cpu": "generic",
  // Optimization: "O0", "O1", "O2", "O3", "O4", "O5", "Os", "Oz".
  "opt": "O0",
}